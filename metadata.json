{
  "name": "rehan-nginx",
  "version": "0.1.0",
  "author": "Rehan Mahmood",
  "summary": "A Puppet module to install, configure and deploy the latest Nginx webserver from stable or mainline.",
  "license": "Apache-2.0",
  "source": "https://github.com/rehanone/puppet-nginx.git",
  "project_page": "https://github.com/rehanone/puppet-nginx",
  "issues_url": "https://github.com/rehanone/puppet-nginx/issues",
  "tags": [
    "Webserver",
    "nginx",
    "reserse proxy"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian"
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "15.10",
        "16.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.2.0 < 3.0.0"
    }
  ],
  "data_provider": null
}
